                  <retailDoc:documentation>This field will hold the translated text of the primary description of the item.</retailDoc:documentation>
               </retailDoc:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="short_desc" type="varchar2120">
               <retailDoc:annotation>
